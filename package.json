{
	"name": "al-navigator",
	"displayName": "AL Navigator",
	"description": "Various functions to help navigating through al objects and developing extensions",
	"version": "0.1.3",
	"publisher": "wbrakowski",
	"author": {
		"name": "Waldemar Brakowski",
		"email": "wbrakowski@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wbrakowski/al-navigator"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.DataItem",
		"onCommand:extension.OnAfterGetRecord",
		"onCommand:extension.Trigger",
		"onCommand:extension.Keys",
		"onCommand:extension.Actions",
		"onLanguage:al"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.DataItem",
				"title": "Jump: Next DataItem"
			},
			{
				"command": "extension.OnAfterGetRecord",
				"title": "Jump: Next OnAfterGetRecord"
			},
			{
				"command": "extension.Trigger",
				"title": "Jump: Next trigger"
			},
			{
				"command": "extension.Keys",
				"title": "Jump: Keys section"
			},
			{
				"command": "extension.Actions",
				"title": "Jump: Actions section"
			},
			{
				"command": "extension.LastLocalVarLine",
				"title": "Jump: Last local var line"
			}
		],
		"keybindings": [
			{
				"command": "extension.DataItem",
				"key": "ctrl+alt+d"
			},
			{
				"command": "extension.OnAfterGetRecord",
				"key": "ctrl+alt+g"
			},
			{
				"command": "extension.Trigger",
				"key": "ctrl+alt+t"
			},
			{
				"command": "extension.Keys",
				"key": "ctrl+alt+k"
			},
			{
				"command": "extension.Actions",
				"key": "ctrl+alt+a"
			},
			{
				"command": "extension.LastLocalVarLine",
				"key": "ctrl+alt+l"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
