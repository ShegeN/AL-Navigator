{
	"name": "al-navigator",
	"displayName": "AL Navigator",
	"description": "Create variables like a boss and navigate through al files with awesome shortcuts.",
	"version": "0.3.3",
	"publisher": "wbrakowski",
	"license": "MIT",
	"url": "https://github.com/wbrakowski/al-navigator",
	"icon": "resources/alpaca-icon_128_128.png",
	"author": {
		"name": "Waldemar Brakowski",
		"email": "wbrakowski@gmail.com"
	},
	"keywords": [
		"AL",
		"AL Extensions",
		"Business Central",
		"CISDE",
		"Dynamics NAV"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/wbrakowski/al-navigator"
	},
	"bugs": {
		"email": "wbrakowski@gmail.com",
		"url": "https://github.com/wbrakowski/AL-Navigator/issues"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.DataItem",
		"onCommand:extension.DataItemBottom",
		"onCommand:extension.OnAfterGetRecord",
		"onCommand:extension.Trigger",
		"onCommand:extension.Keys",
		"onCommand:extension.Actions",
		"onLanguage:al"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.DataItem",
				"title": "AL Navigator: Next DataItem (starting from top)"
			},
			{
				"command": "extension.DataItemBottom",
				"title": "AL Navigator: Next DataItem (starting from bottom)"
			},
			{
				"command": "extension.OnAfterGetRecord",
				"title": "AL Navigator: Next OnAfterGetRecord"
			},
			{
				"command": "extension.Trigger",
				"title": "AL Navigator: Next Trigger"
			},
			{
				"command": "extension.OnDelete",
				"title": "AL Navigator: OnDelete"
			},
			{
				"command": "extension.OnModify",
				"title": "AL Navigator: OnModify"
			},
			{
				"command": "extension.OnInsert",
				"title": "AL Navigator: OnInsert"
			},
			{
				"command": "extension.Keys",
				"title": "AL Navigator: Keys Section"
			},
			{
				"command": "extension.Actions",
				"title": "AL Navigator: Next Action"
			},
			{
				"command": "extension.LastLocalVarLine",
				"title": "AL Navigator: Last local var line"
			},
			{
				"command": "extension.LastGlobalVarLine",
				"title": "AL Navigator: Last global var line"
			}
		],
		"keybindings": [
			{
				"command": "extension.DataItem",
				"key": "ctrl+alt+d"
			},
			{
				"command": "extension.DataItemBottom",
				"key": "shift+alt+d"
			},
			{
				"command": "extension.OnAfterGetRecord",
				"key": "ctrl+alt+j"
			},
			{
				"command": "extension.Trigger",
				"key": "ctrl+alt+t"
			},
			{
				"command": "extension.OnModify",
				"key": "ctrl+o ctrl+m"
			},
			{
				"command": "extension.OnDelete",
				"key": "ctrl+o ctrl+d"
			},
			{
				"command": "extension.OnInsert",
				"key": "ctrl+o ctrl+i"
			},
			{
				"command": "extension.Keys",
				"key": "ctrl+alt+k"
			},
			{
				"command": "extension.Actions",
				"key": "ctrl+alt+a"
			},
			{
				"command": "extension.LastLocalVarLine",
				"key": "ctrl+alt+l"
			},
			{
				"command": "extension.LastGlobalVarLine",
				"key": "ctrl+alt+g"
			}
		],
		"configuration": [
			{
				"type": "object",
				"title": "AL Navigator Configuration",
				"properties": {
					"alNavigator.ignoreALPrefix": {
						"type": "string",
						"default": "",
						"description": "Remove prefix's from suggested variable names."
					},
					"alNavigator.ignoreALSuffix": {
						"type": "string",
						"default": "",
						"description": "Remove suffix's from suggested variable names."
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"extensionDependencies": [
		"andrzejzwierzchowski.al-code-outline"
	],
	"dependencies": {}
}